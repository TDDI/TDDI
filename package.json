    "browserify": "~10.2.1",
    "gulp-uglify": "~1.2.0",
    "vinyl-source-stream": "~1.1.0",
    "vinyl-buffer": "~1.0.0",
    "reactify": "~1.1.1",
    "gulp-nodemon": "~2.0.3",
    "gulp-minify-css": "~1.1.1",
    "gulp-concat": "~2.5.2",
    "react-tools": "~0.13.3",
    "babel": "~5.4.7",
    "jsdom": "~3.1.2",
    "request": "~2.56.0"
  }
